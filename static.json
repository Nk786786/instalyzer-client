{
    "root": "build/",
    "routes": {
        "/**": "index.html"
    },
    "https_only": true,
    "clean_urls": true,
    "headers": {
        "/**": {
          "Referrer-Policy": "no-referrer, strict-origin-when-cross-origin",
          "Strict-Transport-Security": "max-age=63072000; includeSubDomains",
          "X-Content-Type-Options": "nosniff",
          "X-Frame-Options": "DENY",
          "X-XSS-Protection": "1; mode=block"
        }
    }
}